{
    import "Wizard";
    import "Arch";
    import "Mode";
    import "Installation";
    import "Storage";

    include "installation_ui.ycp";

    Arch::i386 = true;
    Mode::test = true;
    Mode::boot = `FAT;
    Installation::tmpdir = "/tmp";
    Storage::SetPartMode( "USE_DISK" );
    Storage::SetPartDisk( "/dev/hdd" );

    map targets = $[
	"/dev/hdd":
	$[
	  "bus" : "IDE", "cyl_count" : 256, "cyl_size" : 5160960,
	  "device" : "/dev/hdc", "name" : "4. IDE, 25.49 GB, /dev/hdd, (full)",
	  "partitions" : [
		  $[
		    "delete" : false,
		    "fsid" : 6,
		    "fstype" : "DOS",
		    "nr" : 1,
		    "region" : [
		      0,
		      249
		    ],
		    "type" : `primary,
		    "ui_id" : 1
		  ],
		  $[
		    "region" : [
		      249,
		      7
		    ],
		    "type" : `free,
		    "ui_id" : 2
		  ]
	  ],
	  "vendor" : "WDC"
	]
    ];

    boolean config_mode = false;

    Storage::SetTargetMap( targets );

    Wizard::CreateDialog();
    WFM::CallFunction (`inst_target_part (true, true, false));
    UI::CloseDialog();
}

